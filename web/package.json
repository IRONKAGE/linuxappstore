{
  "name": "linux-app-store",
  "description": "A Universal App Store for Linux",
  "version": "0.4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linuxappstore/linuxappstore-frontend.git"
  },
  "main": "src/main.js",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^5.0.1"
  },
  "author": {
    "name": "Chad Adams",
    "email": "nshusa99@gmail.com"
  },
  "build": {
    "appId": "io.linuxappstore",
    "productName": "Linux App Store",
    "files": [
      "build/**/*",
      "package.json"
    ],
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Utility"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "pack": "electron-builder --dir",
    "package": "yarn run build && electron-builder build --publish never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "react-scripts": "^3.0.1",
    "typescript": "^3.5.3"
  }
}
